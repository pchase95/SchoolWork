import java.io.*;
import java.util.*;

public class SolutionMain
{
	private IHeap<customer> heap = new Heap<customer>();
	
	public SolutionMain()
	{
		readFile();
	}

	public static void main(String[] args) 
	{
		new SolutionMain();
	}
	
	
	
	public void readFile()
	{
	  String line = "";
	  try
	  {
	         Scanner sr = new Scanner(new File("customers.txt"));
	         while (sr.hasNextLine())
	         {
	        	 line = sr.nextLine();
	        	 String[] part = line.split(", ");
	        	 new customer(part[0], Integer.parseInt(part[1]), Integer.parseInt(part[2]), Integer.parseInt(part[3]));
	         }       
	      }
	  catch(Exception e)
	  {
		  System.out.println("File not found");
	  }
	}
}

class customer
{
	String name;
	int money;
	int years;
	int order;
	
	private customer(String name, int money, int years, int order)
	{
		name = this.name;
		money = this.money;
		years = this.years;
		order = this.order;
	}
}


