import java.io.*;

public class SolutionMain
{
	
	public SolutionMain()
	{
		readFile();
	}

	public static void main(String[] args) 
	{
		new SolutionMain();
	}
	
	
	
	public void readFile()
	{
	  String line = null;
	  try
	  {
	         BufferedReader br = new BufferedReader("customers.txt");
	         while ((line = br.readLine()) != null)
	         {
	            System.out.println(line);
	         }       
	      }
	  catch(Exception e)
	  {
		  e.printStackTrace();
	  }
	}
}
