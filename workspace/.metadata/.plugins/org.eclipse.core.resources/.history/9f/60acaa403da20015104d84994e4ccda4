import java.io.*;
import java.util.*;

public class SolutionMain
{
	private IHeap<Integer> heap = new Heap<Integer>();
	private ArrayList<Customer> customers = new ArrayList<Customer>();
	
	public SolutionMain()
	{
		readFile();
		Customer cust = customers[0];
		System.out.println(customers);
	}

	public static void main(String[] args) 
	{
		new SolutionMain();
	}
	
	
	
	public void readFile()
	{
	  String line = "";
	  try
	  {
	         Scanner sr = new Scanner(new File("customers.txt"));
	         while (sr.hasNextLine())
	         {
	        	 line = sr.nextLine();
	        	 String[] part = line.split(", ");
	        	 customers.add(new Customer(part[0], Integer.parseInt(part[1]), Integer.parseInt(part[2]), Integer.parseInt(part[3])));
	         }       
	      }
	  catch(Exception e)
	  {
		  System.out.println("File not found");
	  }
	}
	private class Customer<T>
	{
		String name;
		int money;
		int years;
		int order;
		
		private Customer(String name, int money, int years, int order)
		{
			name = this.name;
			money = this.money;
			years = this.years;
			order = this.order;
		}
		
		public String toString()
		{
			return name;
		}
	}
}




