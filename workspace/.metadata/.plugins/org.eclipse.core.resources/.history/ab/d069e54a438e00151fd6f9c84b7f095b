import javax.swing.*;
import java.awt.event.*;
import java.awt.*;

public class GUI extends JPanel
{
	private JFrame f;
	private JPanel p;
	
	public GUI()
	{
		f = new JFrame("StuffStore");
		f.setVisible(true);
		f.setSize(600, 400);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		p = new JPanel();
	}
	
	
	
}

